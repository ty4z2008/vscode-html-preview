{
  "version": "0.1.0",
  "configurations": [{
    "name": "preview html",
    "type": "node",
    "request": "launch",
    "args": [
      "${workspaceRoot}/src/extension.ts"
    ],
    "runtimeArgs": [
      "-r",
      "ts-node/register"
    ],
    "cwd": "${workspaceRoot}",
    "protocol": "inspector",
    "internalConsoleOptions": "openOnSessionStart",
    "env": {
      "DEBUG": "*",
      "NODE_ENV": "development",
      "TS_NODE_IGNORE": "false",
      "TS_NODE_PROJECT": "${workspaceRoot}/tsconfig.json"
    }
  }]
}